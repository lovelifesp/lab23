 --10
 DELETE FROM order_details WHERE quantity = 1;

 --11
 SELECT AVG(quantity)FROM order_details; SELECT MAX(quantity)FROM order_details; SELECT MIN(quantity)
 FROM order_details;

 --12
 SELECT AVG(quantity)FROM order_details GROUP BY orderid; SELECT MAX(quantity)FROM order_details GROUP BY orderid;
 SELECT MIN(quantity)FROM order_details GROUP BY orderid;

 --13
